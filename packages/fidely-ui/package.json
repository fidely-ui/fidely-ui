{
  "name": "@fidely-ui/react",
  "version": "1.0.1-beta.1",
  "private": false,
  "description": "A beautifully crafted React design system powered by Ark UI and Panda CSS, providing accessible, themeable, and production-ready components.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.cjs"
    },
    "./collection": {
      "types": "./dist/collection.d.ts",
      "import": "./dist/collection.js",
      "require": "./dist/collection.cjs"
    },
    "./anatomy": {
      "types": "./dist/anatomy.d.ts",
      "import": "./dist/anatomy.js",
      "require": "./dist/anatomy.cjs"
    },
    "./provider": {
      "types": "./dist/provider/index.d.ts",
      "import": "./dist/provider/index.js",
      "require": "./dist/provider/index.cjs"
    },
    "./*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.js",
      "require": "./dist/components/*/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rimraf --no-glob ./dist",
    "build": "pnpm clean && tsup --config tsup.config.ts && tsc --outDir dist --project tsconfig.json",
    "prebuild": "pnpm clean",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --config ../../eslint.config.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fidely-ui/fidely-ui",
    "directory": "packages/fidely-ui"
  },
  "bugs": {
    "url": "https://github.com/fidely-ui/fidely-ui/issues"
  },
  "keywords": [
    "Fidely UI",
    "react",
    "ui",
    "ark-ui",
    "panda-css",
    "design-system",
    "react-components",
    "accessible",
    "uikit",
    "components",
    "library"
  ],
  "author": "Justice Chimobi <chimobi.justice@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "peerDependencies": {
    "@pandacss/dev": ">=1.1.0",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@ark-ui/react": "^5.27.1",
    "@pandacss/dev": "^1.4.3",
    "@fidely-ui/panda-preset": "workspace:*",
    "@fidely-ui/styled-system": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-icons": "^5.5.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
